#include <stdio.h>


int main (){

	int num1, num2;
	
		printf ("\n\n\n Atividade ""g"" da LISTA ");
			printf (" \n\n  Digite o primeiro numero: ");
				scanf("%d", &num1);
			printf (" \n\n  Digite o segundo numero: ");
				scanf("%d", &num2);
				
			if (num1 % num2 == 0) {printf (" \n\n O primeiro numero e divisivel pelo segundo.");}
			else {printf ("\n\n O primeiro nao e divisivel pelo segundo.");}	
				
			
	
	
	
	return 0;
}
