#include <stdio.h>

int main(){
	
	float num1 = 0, num2 = 0, num3 = 0, result = 0;
	
		printf("\n\n\n    CALCULADORA DE MEDIAS ARITMETICAS: ")/
			printf ("\n\n   Digite a primeira nota: ");
				scanf("%f", &num1);
			printf ("\n\n   Digite a segunda nota: ");
				scanf("%f", &num2);
			printf ("\n\n   Digite a terceira nota: ");
				scanf("%f", &num3);
				
				
		result = (num1 + num2 + num3) / 3;
		
			printf("\n\n\n\n     O resultado da media aritmetica e de: %2.2f", result);
	
	
	return 0;
}
