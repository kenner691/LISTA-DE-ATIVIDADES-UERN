#include <stdio.h>


int main (){
	
	int num1, num2;
	
	printf ("          Atividade H da lista 1: ");
		printf ("\n\n\n    Digite o primeiro numero: ");
			scanf("%d", &num1);
		printf ("\n\n\n    Digite o segundo numero: ");
			scanf("%d", &num2);
			
			if (num1 > num2){printf ("%d e maior que %d", num1, num2);}
			else if (num2 > num1){printf ("%d e maior que %d", num2, num1);}
			else{printf("Nao foi especificado que o numero poderia ser igual.");
		}
	
	
	return 0;
}
